		<link rel="icon" type="image/svg+xml" href="/resume.png" />
		<link rel="stylesheet" href="styles.css">
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
		<Navbar />
		<!-- <Header /> -->
	<body>
		<slot />
	</body>
</html>
